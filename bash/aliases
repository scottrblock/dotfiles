# Processes
alias tu='top -o cpu' # cpu
alias tm='top -o vsize' # memory

# Git
alias gb='git branch'
alias gba='git branch -a'
alias gc='git commit -v'
alias gcam='git commit -a -m'
alias gco='git checkout'
alias gd='git diff'
alias gdm='git diff master'
alias gl='git pull'
alias gp='git push'
alias gst='git status'
alias gt='git status'
alias g='git status'
alias gaa='git add .'

